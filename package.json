{
  "name": "thor-framework",
  "version": "0.0.1",
  "description": "Node.js framework",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "set-aliases": "link-module-alias",
    "start": "npm run set-aliases && node --experimental-specifier-resolution=node -r dotenv/config index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreysuha2/thor-framework.git"
  },
  "keywords": [
    "nodejs",
    "node.js",
    "node",
    "node",
    "framework"
  ],
  "author": "Andrey Sukhenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andreysuha2/thor-framework/issues"
  },
  "homepage": "https://github.com/andreysuha2/thor-framework#readme",
  "devDependencies": {
    "dotenv": "^8.5.1",
    "link-module-alias": "^1.2.0"
  },
  "_moduleAliases": {
    "core": "core",
    "core-services": "core/services",
    "core-bootstrap": "core/bootstrap",
    "core-helpers": "core/helpers",
    "core-config": "core/config",
    "core-server": "core/server",
    "core-request": "core/server/request",
    "core-response": "core/server/response",
    "core-router": "core/router",
    "core-route": "core/router/register.js",
    "core-middlewares": "core/router/middlewares",
    "core-middleware": "core/router/middlewares/middleware.js",
    "core-request-validator": "core/router/requests",
    "core-controllers": "core/controllers",
    "core-controller": "core/controllers/controller.js",
    "core-cli": "core/cli",
    "core-commands": "core/cli/commands",
    "core-db": "core/database",
    "root": "src",
    "root-config": "src/config",
    "root-routes": "src/routes",
    "root-middlewares": "src/middlewares",
    "root-requests": "src/requests",
    "root-controllers": "src/controllers",
    "root-commands": "src/commands"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "joi": "^17.4.0",
    "mongoose": "^5.12.9",
    "multiparty": "^4.2.2",
    "mysql2": "^2.2.5",
    "output-file": "^2.0.2",
    "sequelize": "^6.6.2"
  },
  "bin": {
    "hello": "bin/index.js"
  }
}

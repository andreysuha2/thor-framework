{
  "name": "thor-framework",
  "version": "0.0.1",
  "description": "Node.js framework",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "set-aliases": "link-module-alias",
    "start": "npm run set-aliases && node --experimental-specifier-resolution=node -r dotenv/config index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreysuha2/thor-framework.git"
  },
  "keywords": [
    "nodejs",
    "node.js",
    "node",
    "node",
    "framework"
  ],
  "author": "Andrey Sukhenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andreysuha2/thor-framework/issues"
  },
  "homepage": "https://github.com/andreysuha2/thor-framework#readme",
  "devDependencies": {
    "dotenv": "^8.5.1",
    "link-module-alias": "^1.2.0"
  },
  "_moduleAliases": {
    "core": "core",
    "core-bootstrap": "core/bootstrap",
    "core-helpers": "core/helpers",
    "core-config": "core/config",
    "core-server": "core/server",
    "core-request": "core/server/request",
    "core-response": "core/server/response"
  },
  "dependencies": {
    "multiparty": "^4.2.2"
  }
}
